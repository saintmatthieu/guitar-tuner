{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch (Linux)",
      "type": "lldb",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "stopOnEntry": false,
      "args": [
        // "testWithMedianFilter=false",
        // "indexOfProcessToLog=51",
        // "testCaseId=testFiles/notes/Fender_Stratocaster_unplugged/iPhone_11/E2.wav | testFiles/noise/Fender_Stratocaster_noise.wav | -60"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch (Windows)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": []
    },
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${workspaceFolder}/eval/out"
    }
  ]
}
